RewriteEngine On
RewriteBase /swepgroup17/

# Direct file access - allow CSS, JS, images, etc.
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^.*$ - [L]

# Direct directory access - allow
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [L]

# Everything else goes to index.php
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]